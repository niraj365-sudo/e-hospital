<%- include('./partials/headerAdmin.ejs'); %>
<%- include('./partials/sideMenuAdmin.ejs'); %>
<style>
#senderavatar{
    border-radius: 100%;
    height: 50px ;
    width: 50px;
  }
 .form-inline{
   margin-left: 60%;
 }
</style>

<div class="scrollable" id ="data">
    <h4 class="title"><i class="far fa-comments"></i> Feedbacks</h4>
    <br>
    <table id="datatable" class="table table-bordered table-dark" BORDER="2" CELLPADDING="2" CELLSPACING="2">
    <thead class="thead-dark">
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Upload time </th>
      <th scope="col">Message</th>
    </tr>
    </thead>
    <tbody>
        <% feedback.forEach(feedback => { %>
            <tr> 
            <td id ="name" class="">
              <img id="senderavatar" src="<%= feedback.senderavatar%>"></img>              
             <b> <%= feedback.name %></b>
            </td>  
            <td id="title" width="10%"><%= feedback.date.toDateString()%></td>
            <td id="message" width="55%" style="font-size:15px;"><b><%= feedback.message%></b></td>
            </tr>
            </tbody>
            <% }) %>
    </table>
  </div>

  